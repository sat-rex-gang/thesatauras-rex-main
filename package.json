{
  "name": "thesatauras-rex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate --schema=prisma/schema.dev.prisma && next dev",
    "build": "node scripts/safe-migrate.js && next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate --schema=prisma/schema.prisma",
    "db:seed": "node prisma/seed.js",
    "db:migrate": "prisma migrate dev --schema=prisma/schema.dev.prisma",
    "db:migrate:prod": "prisma migrate deploy --schema=prisma/schema.prisma",
    "db:generate": "prisma generate --schema=prisma/schema.dev.prisma",
    "db:generate:prod": "prisma generate --schema=prisma/schema.prisma",
    "db:studio": "prisma studio --schema=prisma/schema.dev.prisma",
    "db:studio:prod": "prisma studio --schema=prisma/schema.prisma"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@prisma/client": "^6.15.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "canvas-confetti": "^1.9.4",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "postprocessing": "^6.37.8",
    "react": "19.1.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "react-text-input": "^0.0.8",
    "react-type-animation": "^3.2.0",
    "scroll-lock": "^2.1.5",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "prisma": "^6.15.0",
    "tailwindcss": "^4"
  }
}
